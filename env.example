# CipherFlow Hook Mainnet Fork Tests Environment Configuration
# Copy this file to .env and fill in your actual RPC URLs

# Ethereum Mainnet RPC (Required for fork tests)
MAINNET_RPC_URL=https://eth-mainnet.g.alchemy.com/v2/YOUR_ALCHEMY_KEY
# Alternative providers:
# MAINNET_RPC_URL=https://mainnet.infura.io/v3/YOUR_INFURA_KEY
# MAINNET_RPC_URL=https://ethereum.publicnode.com

# Sepolia Testnet RPC (Optional, for testnet deployment)
SEPOLIA_RPC_URL=https://eth-sepolia.g.alchemy.com/v2/YOUR_ALCHEMY_KEY

# Arbitrum Sepolia (Optional)
ARBITRUM_SEPOLIA_RPC_URL=https://arb-sepolia.g.alchemy.com/v2/YOUR_ALCHEMY_KEY

# Base Sepolia (Optional)
BASE_SEPOLIA_RPC_URL=https://base-sepolia.g.alchemy.com/v2/YOUR_ALCHEMY_KEY

# Etherscan API Keys (For contract verification)
ETHERSCAN_API_KEY=YOUR_ETHERSCAN_API_KEY
ARBISCAN_API_KEY=YOUR_ARBISCAN_API_KEY
BASESCAN_API_KEY=YOUR_BASESCAN_API_KEY

# Note: Never commit your actual .env file to version control!
# The .env file should be in your .gitignore
